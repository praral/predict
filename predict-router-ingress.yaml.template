apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: predict 
  annotations:
    kubernetes.io/ingress.class: "istio"
spec:
  rules:
  - http:
      paths:
      - path: /api/v1/model/predict/tensorflow/linear/a
        backend:
          serviceName: predict-tensorflow-linear-a
          servicePort: 31501 
